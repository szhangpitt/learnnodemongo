<!DOCTYPE html>
<html>
<head>
  <% include partials/head-include  %>    
</head>
<body ng-app="appModule">
   <div ng-controller="AppController">
      <h1><%= title %></h1>
      <p>Welcome to <%= title %></p>
      <!-- <img src="/images/doodle1.jpg" blur-img alt="blur"> -->
      <img src="https://unsplash.imgix.net/reserve/D8ijGd3CSGq4BxJ9EzTf_13976945916_fa0ce84ee3_o.jpg?q=75&fm=jpg&s=2fce49d5652813cba428a0f1cc28c5e8" blur-img bluramount="12" alt="blur">
      <form action="api/upload" method="post" enctype="multipart/form-data">
        <input type="text" name="theName">
        <input type="file" name="theFile">
        <input type="submit">
      </form>

      <div class="list-group">
        <a href="#/{{ job._id }}" class="list-group-item" ng-repeat="job in jobs">
           <h4 class="list-group-item-heading">{{ job.title }}</h4>
           <p class="list-group-item-text">{{ job.description }}</p>
       </a>
   </div>
</div>

</body>
<% include partials/bottom-include  %>
</html>
